#R version 4.3.1
#Load VQI SUPRA data (available with approval through SVS PSO https://www.vqi.org/data-analysis/)
library(readr)
data <- read_csv("supra_data.csv")

#Split data into train (70%) and test (30%) sets
library(caTools)
set.seed(123)
sample <- sample.split(data$MALE_DEATH_1yr, SplitRatio = 0.7)
train <- subset(data, sample == TRUE)
test <- subset(data, sample == FALSE)

#Apply Random Over-Sample Examples (ROSE) for class balance on training set
library(ROSE)
train <- ROSE(MALE_DEATH_1yr ~ ., data = train, N = 16832, seed = 123)$data

#Define predictors
library(dplyr)

predictors_train_preop <- train %>% select(REGIONID, CENTERID, PHYSICIANID, SURGYEAR, SURGMONTH, SURGWEEKDAY, AGE, GENDER, BMI, RACE, ETHNICITY, PRIMARYINSURER, RURAL, ADI_NATRANK_MEDIAN, TRANSFER, HTN, PREOP_DIABETES, PREOP_SMOKING, PRIOR_CAD, PRIOR_CABG_PCI, PRIOR_CHF, COPD, PREOP_DIALYSIS, R_PRIOR_BYPASS, P_SUPRA_BYPASS, P_INFRA_BYPASS, R_PRIOR_PVI, P_SUPRA_PTASTENT, P_INFRA_PTASTENT, R_PRIOR_MAJOR_AMP, PRIOR_MIN_AMP, PRIOR_CEACAS, PRIOR_ANEURREP, LIVINGSTATUS, PREOP_AMBUL, HEMO_L, PREOP_CREAT_UMOL, STRESS, PREOP_DUP, PREOP_CTA, PREOP_MRA, ARTERIO, VMAP, PREOP_ASA, PREOP_P2Y, PREOP_STATIN, PREOP_BETABLOCKER, PREOP_ACE, C_PREOP_ANTICOAG, PREOP_ABI, PROC, GRFT_TYPE, SURGERYSIDE, GRFT_ORIGIN_ARTERY, GRFT_ORIGIN_DIA, GRFT_RECP1_SIDE, GRFT_RECP1_ARTERY, GRFT_RECP1_GRFT_DIA, GRFT_RECP1_GRFT_TYPE, GRFT_RECP2_SIDE, GRFT_RECP2_ARTERY, GRFT_RECP2_GRFT_DIA, GRFT_RECP2_GRFT_TYPE, CC_INFRA_BYPASS, CC_PVI, SYMPT, URGENCY, ASACLASS)

predictors_test_preop <- test %>% select(REGIONID, CENTERID, PHYSICIANID, SURGYEAR, SURGMONTH, SURGWEEKDAY, AGE, GENDER, BMI, RACE, ETHNICITY, PRIMARYINSURER, RURAL, ADI_NATRANK_MEDIAN, TRANSFER, HTN, PREOP_DIABETES, PREOP_SMOKING, PRIOR_CAD, PRIOR_CABG_PCI, PRIOR_CHF, COPD, PREOP_DIALYSIS, R_PRIOR_BYPASS, P_SUPRA_BYPASS, P_INFRA_BYPASS, R_PRIOR_PVI, P_SUPRA_PTASTENT, P_INFRA_PTASTENT, R_PRIOR_MAJOR_AMP, PRIOR_MIN_AMP, PRIOR_CEACAS, PRIOR_ANEURREP, LIVINGSTATUS, PREOP_AMBUL, HEMO_L, PREOP_CREAT_UMOL, STRESS, PREOP_DUP, PREOP_CTA, PREOP_MRA, ARTERIO, VMAP, PREOP_ASA, PREOP_P2Y, PREOP_STATIN, PREOP_BETABLOCKER, PREOP_ACE, C_PREOP_ANTICOAG, PREOP_ABI, PROC, GRFT_TYPE, SURGERYSIDE, GRFT_ORIGIN_ARTERY, GRFT_ORIGIN_DIA, GRFT_RECP1_SIDE, GRFT_RECP1_ARTERY, GRFT_RECP1_GRFT_DIA, GRFT_RECP1_GRFT_TYPE, GRFT_RECP2_SIDE, GRFT_RECP2_ARTERY, GRFT_RECP2_GRFT_DIA, GRFT_RECP2_GRFT_TYPE, CC_INFRA_BYPASS, CC_PVI, SYMPT, URGENCY, ASACLASS)

predictors_train_intraop <- train %>% select(RREGIONID, CENTERID, PHYSICIANID, SURGYEAR, SURGMONTH, SURGWEEKDAY, AGE, GENDER, BMI, RACE, ETHNICITY, PRIMARYINSURER, RURAL, ADI_NATRANK_MEDIAN, TRANSFER, HTN, PREOP_DIABETES, PREOP_SMOKING, PRIOR_CAD, PRIOR_CABG_PCI, PRIOR_CHF, COPD, PREOP_DIALYSIS, R_PRIOR_BYPASS, P_SUPRA_BYPASS, P_INFRA_BYPASS, R_PRIOR_PVI, P_SUPRA_PTASTENT, P_INFRA_PTASTENT, R_PRIOR_MAJOR_AMP, PRIOR_MIN_AMP, PRIOR_CEACAS, PRIOR_ANEURREP, LIVINGSTATUS, PREOP_AMBUL, HEMO_L, PREOP_CREAT_UMOL, STRESS, PREOP_DUP, PREOP_CTA, PREOP_MRA, ARTERIO, VMAP, PREOP_ASA, PREOP_P2Y, PREOP_STATIN, PREOP_BETABLOCKER, PREOP_ACE, C_PREOP_ANTICOAG, PREOP_ABI, PROC, GRFT_TYPE, SURGERYSIDE, GRFT_ORIGIN_ARTERY, GRFT_ORIGIN_DIA, GRFT_RECP1_SIDE, GRFT_RECP1_ARTERY, GRFT_RECP1_GRFT_DIA, GRFT_RECP1_GRFT_TYPE, GRFT_RECP2_SIDE, GRFT_RECP2_ARTERY, GRFT_RECP2_GRFT_DIA, GRFT_RECP2_GRFT_TYPE, CC_INFRA_BYPASS, CC_PVI, SYMPT, URGENCY, ASACLASS, R_ANESTHESIA, ABX, ABX_START, ABX_STOP, SKINPREP, GRFT_ENDART, GRFT_RECP1_ENDART, GRFT_RECP2_ENDART, EBL, TOTALPROCTIME, COMPLETION_DOPPLER, PROC_DUPLEX, COMPLETION_ANGIO)

predictors_test_intraop <- test %>% select(REGIONID, CENTERID, PHYSICIANID, SURGYEAR, SURGMONTH, SURGWEEKDAY, AGE, GENDER, BMI, RACE, ETHNICITY, PRIMARYINSURER, RURAL, ADI_NATRANK_MEDIAN, TRANSFER, HTN, PREOP_DIABETES, PREOP_SMOKING, PRIOR_CAD, PRIOR_CABG_PCI, PRIOR_CHF, COPD, PREOP_DIALYSIS, R_PRIOR_BYPASS, P_SUPRA_BYPASS, P_INFRA_BYPASS, R_PRIOR_PVI, P_SUPRA_PTASTENT, P_INFRA_PTASTENT, R_PRIOR_MAJOR_AMP, PRIOR_MIN_AMP, PRIOR_CEACAS, PRIOR_ANEURREP, LIVINGSTATUS, PREOP_AMBUL, HEMO_L, PREOP_CREAT_UMOL, STRESS, PREOP_DUP, PREOP_CTA, PREOP_MRA, ARTERIO, VMAP, PREOP_ASA, PREOP_P2Y, PREOP_STATIN, PREOP_BETABLOCKER, PREOP_ACE, C_PREOP_ANTICOAG, PREOP_ABI, PROC, GRFT_TYPE, SURGERYSIDE, GRFT_ORIGIN_ARTERY, GRFT_ORIGIN_DIA, GRFT_RECP1_SIDE, GRFT_RECP1_ARTERY, GRFT_RECP1_GRFT_DIA, GRFT_RECP1_GRFT_TYPE, GRFT_RECP2_SIDE, GRFT_RECP2_ARTERY, GRFT_RECP2_GRFT_DIA, GRFT_RECP2_GRFT_TYPE, CC_INFRA_BYPASS, CC_PVI, SYMPT, URGENCY, ASACLASS, R_ANESTHESIA, ABX, ABX_START, ABX_STOP, SKINPREP, GRFT_ENDART, GRFT_RECP1_ENDART, GRFT_RECP2_ENDART, EBL, TOTALPROCTIME, COMPLETION_DOPPLER, PROC_DUPLEX, COMPLETION_ANGIO)

predictors_train_postop <- train %>% select(REGIONID, CENTERID, PHYSICIANID, SURGYEAR, SURGMONTH, SURGWEEKDAY, AGE, GENDER, BMI, RACE, ETHNICITY, PRIMARYINSURER, RURAL, ADI_NATRANK_MEDIAN, TRANSFER, HTN, PREOP_DIABETES, PREOP_SMOKING, PRIOR_CAD, PRIOR_CABG_PCI, PRIOR_CHF, COPD, PREOP_DIALYSIS, R_PRIOR_BYPASS, P_SUPRA_BYPASS, P_INFRA_BYPASS, R_PRIOR_PVI, P_SUPRA_PTASTENT, P_INFRA_PTASTENT, R_PRIOR_MAJOR_AMP, PRIOR_MIN_AMP, PRIOR_CEACAS, PRIOR_ANEURREP, LIVINGSTATUS, PREOP_AMBUL, HEMO_L, PREOP_CREAT_UMOL, STRESS, PREOP_DUP, PREOP_CTA, PREOP_MRA, ARTERIO, VMAP, PREOP_ASA, PREOP_P2Y, PREOP_STATIN, PREOP_BETABLOCKER, PREOP_ACE, C_PREOP_ANTICOAG, PREOP_ABI, PROC, GRFT_TYPE, SURGERYSIDE, GRFT_ORIGIN_ARTERY, GRFT_ORIGIN_DIA, GRFT_RECP1_SIDE, GRFT_RECP1_ARTERY, GRFT_RECP1_GRFT_DIA, GRFT_RECP1_GRFT_TYPE, GRFT_RECP2_SIDE, GRFT_RECP2_ARTERY, GRFT_RECP2_GRFT_DIA, GRFT_RECP2_GRFT_TYPE, CC_INFRA_BYPASS, CC_PVI, SYMPT, URGENCY, ASACLASS, R_ANESTHESIA, ABX, ABX_START, ABX_STOP, SKINPREP, GRFT_ENDART, GRFT_RECP1_ENDART, GRFT_RECP2_ENDART, EBL, TOTALPROCTIME, COMPLETION_DOPPLER, PROC_DUPLEX, COMPLETION_ANGIO, TXFUSION, POSTOP_VASO, EXTUB, ICU, MYOCARDIAL_INFARCTION, POSTOP_STROKE, POSTOP_DYSRHYTHMIA, POSTOP_CHF, RESPIRATORY, POSTOP_RENAL, WCOMP, DC_ASA, DC_STATIN, DC_P2Y, DC_BETABLOCKER, DC_ACE, C_DC_ANTICOAG, TOTAL_LOS, NON_HOME_DC)

predictors_test_postop <- test %>% select(REGIONID, CENTERID, PHYSICIANID, SURGYEAR, SURGMONTH, SURGWEEKDAY, AGE, GENDER, BMI, RACE, ETHNICITY, PRIMARYINSURER, RURAL, ADI_NATRANK_MEDIAN, TRANSFER, HTN, PREOP_DIABETES, PREOP_SMOKING, PRIOR_CAD, PRIOR_CABG_PCI, PRIOR_CHF, COPD, PREOP_DIALYSIS, R_PRIOR_BYPASS, P_SUPRA_BYPASS, P_INFRA_BYPASS, R_PRIOR_PVI, P_SUPRA_PTASTENT, P_INFRA_PTASTENT, R_PRIOR_MAJOR_AMP, PRIOR_MIN_AMP, PRIOR_CEACAS, PRIOR_ANEURREP, LIVINGSTATUS, PREOP_AMBUL, HEMO_L, PREOP_CREAT_UMOL, STRESS, PREOP_DUP, PREOP_CTA, PREOP_MRA, ARTERIO, VMAP, PREOP_ASA, PREOP_P2Y, PREOP_STATIN, PREOP_BETABLOCKER, PREOP_ACE, C_PREOP_ANTICOAG, PREOP_ABI, PROC, GRFT_TYPE, SURGERYSIDE, GRFT_ORIGIN_ARTERY, GRFT_ORIGIN_DIA, GRFT_RECP1_SIDE, GRFT_RECP1_ARTERY, GRFT_RECP1_GRFT_DIA, GRFT_RECP1_GRFT_TYPE, GRFT_RECP2_SIDE, GRFT_RECP2_ARTERY, GRFT_RECP2_GRFT_DIA, GRFT_RECP2_GRFT_TYPE, CC_INFRA_BYPASS, CC_PVI, SYMPT, URGENCY, ASACLASS, R_ANESTHESIA, ABX, ABX_START, ABX_STOP, SKINPREP, GRFT_ENDART, GRFT_RECP1_ENDART, GRFT_RECP2_ENDART, EBL, TOTALPROCTIME, COMPLETION_DOPPLER, PROC_DUPLEX, COMPLETION_ANGIO, TXFUSION, POSTOP_VASO, EXTUB, ICU, MYOCARDIAL_INFARCTION, POSTOP_STROKE, POSTOP_DYSRHYTHMIA, POSTOP_CHF, RESPIRATORY, POSTOP_RENAL, WCOMP, DC_ASA, DC_STATIN, DC_P2Y, DC_BETABLOCKER, DC_ACE, C_DC_ANTICOAG, TOTAL_LOS, NON_HOME_DC)

#Train pre-op models
library(caret)
library(xgboost)
library(ranger)
library(naivebayes)
library(e1071)
library(nnet)
train_control <- trainControl(method = "cv", number = 10) #10-fold cross-validation

#Train XGBoost
XGBgrid <- expand.grid(max_depth = c(2,3,4,5,6,7,8,9), nrounds = (1:10)*50, eta = c(0.4,0.3,0.2,0.1,0.05,0.01,0.001), gamma = c(0,0.1,1,1.5,2), subsample = c(0.5,0.6,0.7,0.8,0.9,1), min_child_weight = c(1,3,5,7,10), colsample_bytree = c(0.5,0.6,0.7,0.8,0.9,1) #XGBoost grid search for hyperparameters
XGB <- train(x = as.matrix(predictors_train_preop), y = train$MALE_DEATH_1yr, data = train, method = "xgbTree", metric = "ROC", trControl = train_control, tuneGrid = XGBgrid)

#Train random forest
RFgrid <- expand.grid(mtry = 2:4, splitrule = "gini", min.node.size = c(10,20))
RF <- train(x = predictors_train_preop, y = train$MALE_DEATH_1yr, data = train, method = "ranger", metric = "ROC", trControl = train_control, tuneGrid = RFgrid)

#Train naive bayes
NBgrid <- expand.grid(usekernel = c(TRUE, FALSE), fL = 0:5, adjust = seq(0,5,by=1))
NB <- train(x = predictors_train_preop, y = as.factor(train$MALE_DEATH_1yr), data = train, method = "naive_bayes", metric = "ROC", trControl = train_control, tuneGrid = NBgrid)

#Train support vector machine
SVMgrid <- expand.grid(C = c(0.001, 0.01, 0.1, 1, 10, 100, 10000))
SVM <- train(x = predictors_train_preop, y = train$MALE_DEATH_1yr, data = train, method = "svmRadial", metric = "ROC", trControl = train_control, tuneGrid = SVMgrid)

#Train artaoficial neural network
NNETgrid <- expand.grid(size = seq(1,10,by=1), decay = seq(0.1,0.5,by=0.1))
NNET <- train(x = predictors_train_preop, y = train$MALE_DEATH_1yr, data = train, method = "nnet", metric = "ROC", trControl = train_control, tuneGrid = NNETgrid)

#Train logistic regression
LRgrid <- expand.grid(C = c(0.001, 0.01, 0.1, 1, 10, 100, 10000))
LR <- train(x = predictors_train_preop, y = train$MALE_DEATH_1yr, data = train, method = "glm", metric = "ROC", trControl = train_control, tuneGrid = LRgrid, family = "binomial")

#Evaluate pre-op models on test set
library(pROC)

XGB_pred <- predict(XGB, as.matrix(predictors_test_preop))
auc(test$MALE_DEATH_1yr, XGB_pred)
ci.auc(test$MALE_DEATH_1yr, XGB_pred)
XGB_pred_0.5 <- ifelse(XGB_pred > 0.5,1,0)
confusionMatrix(as.factor(test$MALE_DEATH_1yr), as.factor(XGB_pred_0.5))

RF_pred <- predict(RF, predictors_test_preop)
auc(test$MALE_DEATH_1yr, RF_pred)
ci.auc(test$MALE_DEATH_1yr, RF_pred)
RF_pred_0.5 <- ifelse(RF_pred > 0.5,1,0)
confusionMatrix(as.factor(test$MALE_DEATH_1yr), as.factor(RF_pred_0.5))

NB_pred <- predict(NB, predictors_test_preop)
auc(test$MALE_DEATH_1yr, NB_pred)
ci.auc(test$MALE_DEATH_1yr, NB_pred)
NB_pred_0.5 <- ifelse(NB_pred > 0.5,1,0)
confusionMatrix(as.factor(test$MALE_DEATH_1yr), as.factor(NB_pred_0.5))

SVM_pred <- predict(SVM, predictors_test_preop)
auc(test$MALE_DEATH_1yr, SVM_pred)
ci.auc(test$MALE_DEATH_1yr, SVM_pred)
SVM_pred_0.5 <- ifelse(SVM_pred > 0.5,1,0)
confusionMatrix(as.factor(test$MALE_DEATH_1yr), as.factor(SVM_pred_0.5))

NNET_pred <- predict(NNET, predictors_test_preop)
auc(test$MALE_DEATH_1yr, NNET_pred)
ci.auc(test$MALE_DEATH_1yr, NNET_pred)
NNET_pred_0.5 <- ifelse(NNET_pred > 0.5,1,0)
confusionMatrix(as.factor(test$MALE_DEATH_1yr), as.factor(NNET_pred_0.5))

LR_pred <- predict(LR, predictors_test_preop)
auc(test$MALE_DEATH_1yr, LR_pred)
ci.auc(test$MALE_DEATH_1yr, LR_pred)
LR_pred_0.5 <- ifelse(LR_pred > 0.5,1,0)
confusionMatrix(as.factor(test$MALE_DEATH_1yr), as.factor(LR_pred_0.5))

#XGBoost selected as best performing model
#Train XGBoost on intra-operative and post-operative data
XGB_preop <- XGB
XGB_intraop <- train(x = as.matrix(predictors_train_intraop), y = train$MALE_DEATH_1yr, data = train, method = "xgbTree", metric = "ROC", trControl = train_control, tuneGrid = XGBgrid)
XGB_postop <- train(x = as.matrix(predictors_train_postop), y = train$MALE_DEATH_1yr, data = train, method = "xgbTree", metric = "ROC", trControl = train_control, tuneGrid = XGBgrid)

#Generate ROC curves
library(ROCR)
XGB_preop_pred <- predict(XGB_preop, as.matrix(predictors_test_preop))
XGB_preop_perf <- prediction(XGB_preop_pred, test$MALE_DEATH_1yr)
XGB_preop_perf <- performance(XGB_preop_perf, "tpr", "fpr")

XGB_intraop_pred <- predict(XGB_intraop, as.matrix(predictors_test_intraop))
XGB_intraop_perf <- prediction(XGB_intraop_pred, test$MALE_DEATH_1yr)
XGB_intraop_perf <- performance(XGB_intraop_perf, "tpr", "fpr")

XGB_postop_pred <- predict(XGB_postop, as.matrix(predictors_test_postop))
XGB_postop_perf <- prediction(XGB_postop_pred, test$MALE_DEATH_1yr)
XGB_postop_perf <- performance(XGB_postop_perf, "tpr", "fpr")

plot(XGB_preop_perf)
plot(XGB_intraop_perf, add = TRUE, col = 'blue')
plot(XGB_postop_perf, add = TRUE, col = 'orange')
abline(a=0,b=1)

#Generate calibration plots
library(gbm)
calibrate.plot(test$MALE_DEATH_1yr, XGB_preop_pred)
calibrate.plot(test$MALE_DEATH_1yr, XGB_intraop_pred)
calibrate.plot(test$MALE_DEATH_1yr, XGB_intraop_pred)

#Calculate Brier Scores
library(DescTools)
BrierScore(XGB_preop_pred, test$MALE_DEATH_1yr)
BrierScore(XGB_intraop_pred, test$MALE_DEATH_1yr)
BrierScore(XGB_postop_pred, test$MALE_DEATH_1yr)

#Calculate variable importance scores for top 10 predictors
importance_matrix <- xgb.importance(names(predictors_test_postop), model = XGB_postop)
xgb.plot.importance(importance_matrix, top_n = 10)

#Subgroup analysis of variable importance scores for top 10 predictors based on symptom status

test_CLTI <- test[test$SYMPT>=2, ]
predictors_test_CLTI <- test_CLTI %>% select(REGIONID, CENTERID, PHYSICIANID, SURGYEAR, SURGMONTH, SURGWEEKDAY, AGE, GENDER, BMI, RACE, ETHNICITY, PRIMARYINSURER, RURAL, ADI_NATRANK_MEDIAN, TRANSFER, HTN, PREOP_DIABETES, PREOP_SMOKING, PRIOR_CAD, PRIOR_CABG_PCI, PRIOR_CHF, COPD, PREOP_DIALYSIS, R_PRIOR_BYPASS, P_SUPRA_BYPASS, P_INFRA_BYPASS, R_PRIOR_PVI, P_SUPRA_PTASTENT, P_INFRA_PTASTENT, R_PRIOR_MAJOR_AMP, PRIOR_MIN_AMP, PRIOR_CEACAS, PRIOR_ANEURREP, LIVINGSTATUS, PREOP_AMBUL, HEMO_L, PREOP_CREAT_UMOL, STRESS, PREOP_DUP, PREOP_CTA, PREOP_MRA, ARTERIO, VMAP, PREOP_ASA, PREOP_P2Y, PREOP_STATIN, PREOP_BETABLOCKER, PREOP_ACE, C_PREOP_ANTICOAG, PREOP_ABI, PROC, GRFT_TYPE, SURGERYSIDE, GRFT_ORIGIN_ARTERY, GRFT_ORIGIN_DIA, GRFT_RECP1_SIDE, GRFT_RECP1_ARTERY, GRFT_RECP1_GRFT_DIA, GRFT_RECP1_GRFT_TYPE, GRFT_RECP2_SIDE, GRFT_RECP2_ARTERY, GRFT_RECP2_GRFT_DIA, GRFT_RECP2_GRFT_TYPE, CC_INFRA_BYPASS, CC_PVI, SYMPT, URGENCY, ASACLASS, R_ANESTHESIA, ABX, ABX_START, ABX_STOP, SKINPREP, GRFT_ENDART, GRFT_RECP1_ENDART, GRFT_RECP2_ENDART, EBL, TOTALPROCTIME, COMPLETION_DOPPLER, PROC_DUPLEX, COMPLETION_ANGIO, TXFUSION, POSTOP_VASO, EXTUB, ICU, MYOCARDIAL_INFARCTION, POSTOP_STROKE, POSTOP_DYSRHYTHMIA, POSTOP_CHF, RESPIRATORY, POSTOP_RENAL, WCOMP, DC_ASA, DC_STATIN, DC_P2Y, DC_BETABLOCKER, DC_ACE, C_DC_ANTICOAG, TOTAL_LOS, NON_HOME_DC)
importance_matrix <- xgb.importance(names(predictors_test_CLTI), model = XGB)
xgb.plot.importance(importance_matrix, top_n = 10)

test_asympt_claud <- test[test$SYMPT<=1, ]
predictors_test_asympt_claud <- test_asympt_claud %>% select(REGIONID, CENTERID, PHYSICIANID, SURGYEAR, SURGMONTH, SURGWEEKDAY, AGE, GENDER, BMI, RACE, ETHNICITY, PRIMARYINSURER, RURAL, ADI_NATRANK_MEDIAN, TRANSFER, HTN, PREOP_DIABETES, PREOP_SMOKING, PRIOR_CAD, PRIOR_CABG_PCI, PRIOR_CHF, COPD, PREOP_DIALYSIS, R_PRIOR_BYPASS, P_SUPRA_BYPASS, P_INFRA_BYPASS, R_PRIOR_PVI, P_SUPRA_PTASTENT, P_INFRA_PTASTENT, R_PRIOR_MAJOR_AMP, PRIOR_MIN_AMP, PRIOR_CEACAS, PRIOR_ANEURREP, LIVINGSTATUS, PREOP_AMBUL, HEMO_L, PREOP_CREAT_UMOL, STRESS, PREOP_DUP, PREOP_CTA, PREOP_MRA, ARTERIO, VMAP, PREOP_ASA, PREOP_P2Y, PREOP_STATIN, PREOP_BETABLOCKER, PREOP_ACE, C_PREOP_ANTICOAG, PREOP_ABI, PROC, GRFT_TYPE, SURGERYSIDE, GRFT_ORIGIN_ARTERY, GRFT_ORIGIN_DIA, GRFT_RECP1_SIDE, GRFT_RECP1_ARTERY, GRFT_RECP1_GRFT_DIA, GRFT_RECP1_GRFT_TYPE, GRFT_RECP2_SIDE, GRFT_RECP2_ARTERY, GRFT_RECP2_GRFT_DIA, GRFT_RECP2_GRFT_TYPE, CC_INFRA_BYPASS, CC_PVI, SYMPT, URGENCY, ASACLASS, R_ANESTHESIA, ABX, ABX_START, ABX_STOP, SKINPREP, GRFT_ENDART, GRFT_RECP1_ENDART, GRFT_RECP2_ENDART, EBL, TOTALPROCTIME, COMPLETION_DOPPLER, PROC_DUPLEX, COMPLETION_ANGIO, TXFUSION, POSTOP_VASO, EXTUB, ICU, MYOCARDIAL_INFARCTION, POSTOP_STROKE, POSTOP_DYSRHYTHMIA, POSTOP_CHF, RESPIRATORY, POSTOP_RENAL, WCOMP, DC_ASA, DC_STATIN, DC_P2Y, DC_BETABLOCKER, DC_ACE, C_DC_ANTICOAG, TOTAL_LOS, NON_HOME_DC)
importance_matrix <- xgb.importance(names(predictors_test_asympt_claud), model = XGB)
xgb.plot.importance(importance_matrix, top_n = 10)

#Subgroup analysis based on age
test_under65 <- test[test$AGE<65, ]
test_over65 <- test[test$AGE>65, ]
predictors_test_under65 <- test_under65 %>% select(REGIONID, CENTERID, PHYSICIANID, SURGYEAR, SURGMONTH, SURGWEEKDAY, AGE, GENDER, BMI, RACE, ETHNICITY, PRIMARYINSURER, RURAL, ADI_NATRANK_MEDIAN, TRANSFER, HTN, PREOP_DIABETES, PREOP_SMOKING, PRIOR_CAD, PRIOR_CABG_PCI, PRIOR_CHF, COPD, PREOP_DIALYSIS, R_PRIOR_BYPASS, P_SUPRA_BYPASS, P_INFRA_BYPASS, R_PRIOR_PVI, P_SUPRA_PTASTENT, P_INFRA_PTASTENT, R_PRIOR_MAJOR_AMP, PRIOR_MIN_AMP, PRIOR_CEACAS, PRIOR_ANEURREP, LIVINGSTATUS, PREOP_AMBUL, HEMO_L, PREOP_CREAT_UMOL, STRESS, PREOP_DUP, PREOP_CTA, PREOP_MRA, ARTERIO, VMAP, PREOP_ASA, PREOP_P2Y, PREOP_STATIN, PREOP_BETABLOCKER, PREOP_ACE, C_PREOP_ANTICOAG, PREOP_ABI, PROC, GRFT_TYPE, SURGERYSIDE, GRFT_ORIGIN_ARTERY, GRFT_ORIGIN_DIA, GRFT_RECP1_SIDE, GRFT_RECP1_ARTERY, GRFT_RECP1_GRFT_DIA, GRFT_RECP1_GRFT_TYPE, GRFT_RECP2_SIDE, GRFT_RECP2_ARTERY, GRFT_RECP2_GRFT_DIA, GRFT_RECP2_GRFT_TYPE, CC_INFRA_BYPASS, CC_PVI, SYMPT, URGENCY, ASACLASS)
