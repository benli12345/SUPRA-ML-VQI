pred_elective <- predict(XGB_preop, as.matrix(predictors_test_elective))
auc(test_elective$MALE_DEATH_1yr, pred_elective)
ci.auc(test_elective$MALE_DEATH_1yr, pred_elective)

perf_urgent <- prediction(pred_urgent, test_urgent$MALE_DEATH_1yr)
perf_urgent <- performance(perf_urgent, "tpr", "fpr")

perf_elective <- prediction(pred_elective, test_elective$MALE_DEATH_1yr)
perf_elective <- performance(perf_elective, "tpr", "fpr")

plot(perf_urgent)
plot(perf_elective, add = TRUE, col = 'blue')
abline(a=0,b=1)

#Train and evaluate XGBoost for predicting secondary outcomes

XGB_pat_preop <- xgboost(data = as.matrix(predictors_train_preop), label = as.numeric(train$pat), nrounds = 200, verbose = TRUE, objective = 'binary:logistic', eval_metric = 'auc', max_depth = 3, eta = 0.3, gamma = 0, colsample_bytree = 0.6, min_child_weight = 1, subsample = 1) #hyperparameters chosen based on above grid search and cross validation
XGB_pat_pred_preop <- predict(XGB_pat_preop, as.matrix(predictors_test_preop))
auc(test$pat, XGB_pat_pred_preop)
ci.auc(test$pat, XGB_pat_pred_preop)
XGB_pat_pred_preop_0.5 <- ifelse(XGB_pat_pred_preop > 0.5,1,0)
confusionMatrix(as.factor(test$pat), as.factor(XGB_pat_pred_preop_0.5))

XGB_pat_intraop <- xgboost(data = as.matrix(predictors_train_intraop), label = as.numeric(train$pat), nrounds = 200, verbose = TRUE, objective = 'binary:logistic', eval_metric = 'auc', max_depth = 3, eta = 0.3, gamma = 0, colsample_bytree = 0.6, min_child_weight = 1, subsample = 1)
XGB_pat_pred_intraop <- predict(XGB_pat_intraop, as.matrix(predictors_test_intraop))
auc(test$pat, XGB_pat_pred_intraop)
ci.auc(test$pat, XGB_pat_pred_intraop)
XGB_pat_pred_intraop_0.5 <- ifelse(XGB_pat_pred_intraop > 0.5,1,0)
confusionMatrix(as.factor(test$pat), as.factor(XGB_pat_pred_intraop_0.5))

